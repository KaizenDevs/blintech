          </div>
          <div class="slider_pagination" id="slider_pag"></div>
        </div>

        <script>
          jQuery(document).ready(function($) {
            $('#slider1').carouFredSel({
              pagination : "#slider_pag",
              infinite: false,
              auto: false,
              height: "auto",
              items: 1,
              scroll: {
                fx: "fade",
                duration: 200
