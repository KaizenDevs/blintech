            <div class="slider-item video_slide">
              <iframe src="<%= @video %>?byline=0&amp;portrait=0&amp;title=0" width="430" height="242" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
            </div>
            <div class="slider-item">

              <a href="#">
                <img src="<%= render inline: @page.image5 %>" class="frame_right" alt="" id="page_image5" data-mercury="image">
              </a>
            </div>
            <div class="slider-item text_slide">
              <div class="inner">
